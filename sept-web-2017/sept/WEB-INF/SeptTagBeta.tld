<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xmls/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xmls/j2ee http://java.sun.com/xmls/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<description>A tag library exercising SimpleTag handlers.</description>
	<tlib-version>1.0</tlib-version>
	<short-name>FreamworkCenter</short-name>
	<uri>/SeptTagBeta</uri>
	<!-- scriptless 仅仅能包含自定义 -->
	<!-- 总body标签 -->
	<tag>
		<description>总父标签,可作为单独页面使用</description>
		<name>body</name>
		<tag-class>com.sept.framework.taglib.sept.body.BodyTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description> 默认 ANT-TAGLIB-API</description>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description> 默认store/ico/ant.ico</description>
			<name>pageIcon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description> 页面描述</description>
			<name>description</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>页面关键词</description>
			<name>keywords</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>是否主頁，默认非主页</description>
			<name>root</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>布局类型，relative(相对布局),absolute(绝对布局),horizontal(横向布局),vertical(纵向布局)
				均可写从前往后的任意长度字符，在使用自定义标签的情况下，默认vertical，前两个不生效，写原声html的情况下，默认absolute，后两个不生效
			</description>
			<name>layout</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>主题</description>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- 原生html代码编写区域 -->
	<tag>
		<description>html编写区</description>
		<name>diy</name>
		<tag-class>com.sept.framework.taglib.sept.diy.DiyTag</tag-class>
		<body-content>JSP</body-content>
	</tag>


	<tag>
		<description>layout</description>
		<name>layout</name>
		<tag-class>com.sept.framework.taglib.sept.layout.LayoutTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>id</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>region 在折叠区域面板的时候触发。</description>
			<name>onCollapse</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>region 在展开区域面板的时候触发。</description>
			<name>onExpand</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>region 在新增区域面板的时候触发</description>
			<name>onAdd</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>region 在移除区域面板的时候触发。</description>
			<name>onRemove</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>是否隐藏</description>
			<name>hidden</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>layoutcell</description>
		<name>layoutCell</name>
		<tag-class>com.sept.framework.taglib.sept.layout.layoutCell.LayoutCellTag
		</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>是否隐藏</description>
			<name>hidden</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>位置north, south, east, west, center</description>
			<name>region</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>布局面板标题文本。</description>
			<name>labelValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>id</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>为true时显示布局面板边框。</description>
			<name>border</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>为true时用户可以通过分割栏改变面板大小。</description>
			<name>split</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>一个包含图标的CSS类ID，该图标将会显示到面板标题上。</description>
			<name>iconCls</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>加载地址</description>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>定义是否显示折叠按钮。</description>
			<name>collapsible</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>最小面板宽度</description>
			<name>minWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>最小面板高度</description>
			<name>minHeight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>最大面板宽度</description>
			<name>maxWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>最大面板高度。</description>
			<name>maxHeight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>在点击折叠面板时候的扩展模式。可用值有：“float”、“dock”和null
				float：区域面板将展开并浮动在顶部，在鼠标焦点离开面板时会自动隐藏。
				dock：区域面板将展开并钉在面板上，在鼠标焦点离开面板时不会自动隐藏。
				null：什么也不会发生。
			</description>
			<name>expandMode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>折叠后的面板大小。</description>
			<name>collapsedSize</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>为true时隐藏折叠面板上的扩展面板工具。</description>
			<name>hideExpandTool</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>为true时隐藏折叠面板上的标题栏。</description>
			<name>hideCollapsedContent</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>不知道怎么用，先研究一下，会用自己先用</description>
			<name>collapsedContent</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<description>tab</description>
		<name>tab</name>
		<tag-class>com.sept.framework.taglib.sept.tab.TabTag
		</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>是否隐藏</description>
			<name>hidden</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>id</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>设置为true时，将不显示控制面板背景。 false</description>
			<name>plain</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description> 设置为true时，选项卡的大小将铺满它所在的容器 默认true</description>
			<name>fit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>设置为true时，显示选项卡容器边框。 true</description>
			<name>border</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡滚动条每次滚动的像素值。 100</description>
			<name>scrollIncrement</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>number 每次滚动动画持续的时间，单位：毫秒。 400</description>
			<name>scrollDuration</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>工具栏位置。可用值：'left','right'。</description>
			<name>toolPosition</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡位置。可用值：'top','bottom','left','right'。</description>
			<name>tabPosition</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡标题宽度，在tabPosition属性设置为'left'或'right'的时候才有效。 150
			</description>
			<name>headerWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>标签条的宽度。</description>
			<name>tabWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>标签条的高度。</description>
			<name>tabHeight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>初始化选中一个标签页。</description>
			<name>selected</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>设置为true时，显示标签页标题</description>
			<name>showHeader</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>设置为true时，生成等宽标题选项卡</description>
			<name>justified</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>设置为true时，删除选项卡标题之间的空间 默认false</description>
			<name>narrow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>设置为true时，选项卡标题样式改为气泡状 默认false</description>
			<name>pill</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>panel 在ajax选项卡面板加载完远程数据的时候触发。</description>
			<name>onLoad</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description> title,index 用户在选择一个选项卡面板的时候触发。</description>
			<name>onSelect</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description> title,index 用户在取消选择一个选项卡面板的时候触发</description>
			<name>onUnselect</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>title,index 在选项卡面板关闭的时候触发，返回false取消关闭操作</description>
			<name>onBeforeClose</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>title,index 在用户关闭一个选项卡面板的时候触发。</description>
			<name>onClose</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<description> title,index 在添加一个新选项卡面板的时候触发。</description>
			<name>onAdd</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description> title,index 在更新一个选项卡面板的时候触发</description>
			<name>onUpdate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>在右键点击一个选项卡面板的时候触发 e, title,index</description>
			<name>onContextMenu</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>tabPage</description>
		<name>tabPage</name>
		<tag-class>com.sept.framework.taglib.sept.tab.TabPageTag
		</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>是否隐藏</description>
			<name>hidden</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>id</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡面板的标题文本映射到title上</description>
			<name>labelValue</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡面板的内容 映射到href上</description>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡面板的内容。</description>
			<name>content</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>如果为true，在'url'属性设置了有效值的时候缓存选项卡面板。 true</description>
			<name>cache</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>定义了一个图标的CSS类ID显示到选项卡面板标题</description>
			<name>iconCls</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>如果为true，则允许选项卡摺叠。 false</description>
			<name>collapsible</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>在设置为true的时候，选项卡面板将显示一个关闭按钮，在点击的时候会关闭选项卡面板。 false
			</description>
			<name>closable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>在设置为true的时候，选项卡面板会被选中 。false</description>
			<name>selected</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>在设置为true的时候，选项卡面板会被禁用。false</description>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>按钮</description>
		<name>button</name>
		<tag-class>com.sept.framework.taglib.sept.button.ButtonTag
		</tag-class>
