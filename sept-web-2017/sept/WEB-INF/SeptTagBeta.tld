		<attribute>
			<description>id</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>选项卡面板的标题文本映射到title上</description>
			<name>labelValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>定义了一个图标的CSS类ID显示到选项卡面板标题</description>
			<name>iconCls</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
